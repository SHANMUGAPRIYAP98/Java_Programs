package com.srm.oops;
class Employee
{
	int empId;
	String empName,addr,mailId;
	long mobileNo;
	public Employee(int empId,String empName,String addr,String mailId,long mobileNo) {
		this.empId=empId;
		this.empName=empName;
		this.addr=addr;
		this.mailId=mailId;
		this.mobileNo=mobileNo;
	}
	
}
