package com.srm.basics;

import java.util.Scanner;

class ArrayDemo
{
	
}
public class ArrayOper {
	public static void main(String[] args) {
		System.out.println("Enter the No.of.Elements : ");
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int[] arr=new int[n];
		for(int i=0;i<n;i++)
		{
			arr[i]=sc.nextInt();
		}

	}

}
