package com.srm.oops;

class Point
{
	private int x,y;
	public Point()
	{
		this.x=x;
		this.y=y;
	}
	public Point(int x,int y)
	{
		this.x=x;
		this.y=y;
	}
	public void setX(int x)
	{
		this.x=x;
	}
	public void setY(int y)
	{
		this.y=y;
	}
}
public class PointDemo {

	public static void main(String[] args) {
		

	}

}
